@model Shooping.Data.Entities.State

@{
    ViewData["Title"] = "Delete";
}

<h1>Borrar</h1>

<h3>¿Está seguro de borrar el registro?</h3>
<div>

    <partial name="_StateDetails"/>
    
    <form asp-action="DeleteState">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-outline-danger" /> 
        <a asp-action="Details" asp-route-for="@Model.Country.Id"  class="btn btn-outline-success">Regresar</a>
    </form>
</div>
